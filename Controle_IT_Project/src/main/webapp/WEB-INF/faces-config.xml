<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <managed-bean>
        <managed-bean-name>projectController</managed-bean-name>
        <managed-bean-class>com.demo30.controller.ProjectController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <!-- Define navigation rules -->
    <navigation-rule>
        <from-view-id>/project-list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit-project</from-outcome>
            <to-view-id>/edit-project.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>project-list</from-outcome>
            <to-view-id>/project-list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <!-- Add more navigation rules as needed -->
</faces-config>